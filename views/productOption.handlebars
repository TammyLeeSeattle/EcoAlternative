<div class="container">
  <h4>Product Option</h4>
</div>

<div class="container">
  <div class="row">
    <div class="col s12 m6">
      <div class="card #8bc34a light-green">
        <div class="card-content white-text">
          <span class="card-title">{{product_name}}Moisturizer</span>
          <p>description</p>
        </div>
        <div class="card-action">
          <a href="#">Action 1</a>
          <a href="#">Action 2</a>
        </div>
      </div>
    </div>
  </div>
</div>


{{#each products}}





<ul>
  
  <li>{{id}}</li>
  <li>{{product_name}}</li>

  <li>{{product.ingredients}}</li>
  <li>{{product_Id}}</li>
  <li>{{ingredients}}</li>
  <li>{{plastic_free}}</li>
  <li>{{reusable}}</li>
  <li>{{price}}</li>
  <li>{{stylish}}</li>
  <li>{{environmental_impact}}</li>
  <li>{{external_link}}</li>
  <li>{{fun_fact}}</li>
  <li>{{source}}</li>

  {{!-- <li>{{image}}</li> --}}

  
</ul>


{{/each}}